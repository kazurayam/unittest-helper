<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%msg%n</pattern>
        </encoder>
    </appender>
    <logger name="com.kazurayam.unittest" level="DEBUG" />
    <logger name="com.kazurayam.unittesthelperdemo" level="DEBUG" />
    <root>
        <appender-ref ref="STDOUT" />
    </root>
</configuration>